---
import { URL_BASE } from '../config.js';
---

<header class="w-full bg-personalizado p-8 fixed h-28 flex items-center z-10 top-0">
  <a class="block p-1" href={URL_BASE}><img class="w-40 h-auto" src={URL_BASE + "img/abbott.webp"} alt="logo abbott"></a>
</header>


<style>
  .bg-personalizado {
    background: rgb(91,32,148);
    background: linear-gradient(45deg, rgba(91,32,148,1) 0%, rgba(169,80,165,1) 49%, rgba(233,154,176,1) 100%);
  }
</style>